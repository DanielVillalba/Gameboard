<!-- Index.cshtml -->
{{2+2}}


<div ng-controller="IndexController">
    {{greeting}}

    <table>
        <tr>
            <th>ID</th>
            <th>Name</th>
            <th>Description</th>
            <th>AgeRestriction</th>
            <th>Company</th>
            <th>Price</th>
        </tr>
        <tbody data-ng-repeat="prod in products">
            <tr>
                <td>{{prod.Id}}</td>
                <td>{{prod.Name}}</td>
                <td>{{prod.Description}}</td>
                <td>{{prod.AgeRestriction}}</td>
                <td>{{prod.Company}}</td>
                <td>{{prod.Price}}</td>
            </tr>
        </tbody>
    </table>

</div>

<div ng-view></div>

